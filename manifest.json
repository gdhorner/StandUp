{
  "manifest_version": 3,
  "name": "Stand Up",
  "description": "A consistent reminder to stand up throughout the day to encourage an active and healthy lifestyle.",
  "version": "1.0",
  "icons": {
    "16": "images/standup.png",
    "32": "images/standup.png",
    "48": "images/standup.png",
    "128": "images/standup.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/standup.png",
      "32": "images/standup.png",
      "48": "images/standup.png",
      "128": "images/standup.png"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "alarms", "notifications"]
}
